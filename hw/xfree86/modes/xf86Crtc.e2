        xf86CrtcConfigPrivateIndex = xf86AllocateScrnInfoPrivateIndex();
    config = XNFcallocarray((1), (sizeof(xf86CrtcConfigRec)));

    config->funcs = funcs;
    config->compat_output = -1;

    scrn->privates[xf86CrtcConfigPrivateIndex].ptr = config;
}

