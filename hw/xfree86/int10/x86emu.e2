            return _X86EMU_env.x86.gen.A.I32_reg.e_reg;
        case 1:
            if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf("[ECX]");
            return _X86EMU_env.x86.gen.C.I32_reg.e_reg;
        case 2:
            if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf("[EDX]");
            return _X86EMU_env.x86.gen.D.I32_reg.e_reg;
        case 3:
            if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf("[EBX]");
            return _X86EMU_env.x86.gen.B.I32_reg.e_reg;
        case 4:
            sib = fetch_byte_imm();
