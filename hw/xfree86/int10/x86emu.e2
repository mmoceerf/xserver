        if (cf) (_X86EMU_env.x86.spc.FLAGS |= (0x0001)); else (_X86EMU_env.x86.spc.FLAGS &= ~(0x0001));
        /* OVERFLOW is set *IFF* cnt==1, then it is the
           xor of CF and the most significant bit.  Blecck. */
        /* parenthesized... */
        if (cnt == 1) {
            if ((((ocf + ((d >> 6) & 0x2)) ^ ((ocf + ((d >> 6) & 0x2))>>1)) & 0x1)) (_X86EMU_env.x86.spc.FLAGS |= (0x0800)); else (_X86EMU_env.x86.spc.FLAGS &= ~(0x0800));
        }
