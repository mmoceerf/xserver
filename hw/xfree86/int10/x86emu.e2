/* Define some packed structures to use with unaligned accesses */

struct __una_u64 {
    x86emuu64 x __attribute__ ((packed));
};
struct __una_u32 {
    x86emuu32 x __attribute__ ((packed));
};
struct __una_u16 {
    x86emuu16 x __attribute__ ((packed));
};

/* Elemental unaligned loads */

static __inline__ x86emuu64
ldq_u(x86emuu64 * p)
{
    const struct __una_u64 *ptr = (const struct __una_u64 *) p;

    return ptr->x;
}

static __inline__ x86emuu32
ldl_u(x86emuu32 * p)
{
    const struct __una_u32 *ptr = (const struct __una_u32 *) p;

    return ptr->x;
}

static __inline__ x86emuu16
ldw_u(x86emuu16 * p)
{
    const struct __una_u16 *ptr = (const struct __una_u16 *) p;

    return ptr->x;
}

/* Elemental unaligned stores */

static __inline__ void
stq_u(x86emuu64 val, x86emuu64 * p)
{
    struct __una_u64 *ptr = (struct __una_u64 *) p;

    ptr->x = val;
}

static __inline__ void
stl_u(x86emuu32 val, x86emuu32 * p)
{
    struct __una_u32 *ptr = (struct __una_u32 *) p;

    ptr->x = val;
}

static __inline__ void
stw_u(x86emuu16 val, x86emuu16 * p)
{
    struct __una_u16 *ptr = (struct __una_u16 *) p;

    ptr->x = val;
}
