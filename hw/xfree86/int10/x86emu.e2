        case 2:
            if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf("[BP+SI]");
            _X86EMU_env.x86.mode |= 0x00000001;
            return (_X86EMU_env.x86.spc.BP.I16_reg.x_reg + _X86EMU_env.x86.spc.SI.I16_reg.x_reg) & 0xffff;
        case 3:
            if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf("[BP+DI]");
            _X86EMU_env.x86.mode |= 0x00000001;
            return (_X86EMU_env.x86.spc.BP.I16_reg.x_reg + _X86EMU_env.x86.spc.DI.I16_reg.x_reg) & 0xffff;
        case 4:
            if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf("[SI]");
            return _X86EMU_env.x86.spc.SI.I16_reg.x_reg;
        case 5:
            if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf("[DI]");
            return _X86EMU_env.x86.spc.DI.I16_reg.x_reg;
