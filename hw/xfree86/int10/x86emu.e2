/****************************************************************************
REMARKS:
Implements the AAA instruction and side effects.
****************************************************************************/
x86emuu16
aaa_word(x86emuu16 d)
{
    x86emuu16 res;

    if ((d & 0xf) > 0x9 || (_X86EMU_env.x86.spc.FLAGS & (0x0010))) {
        d += 0x6;
        d += 0x100;
        (_X86EMU_env.x86.spc.FLAGS |= (0x0010));
        (_X86EMU_env.x86.spc.FLAGS |= (0x0001));
    }
    else {
        (_X86EMU_env.x86.spc.FLAGS &= ~(0x0001));
        (_X86EMU_env.x86.spc.FLAGS &= ~(0x0010));
    }
    res = (x86emuu16) (d & 0xFF0F);
    (_X86EMU_env.x86.spc.FLAGS &= ~(0x0080));
    if (res == 0) (_X86EMU_env.x86.spc.FLAGS |= (0x0040)); else (_X86EMU_env.x86.spc.FLAGS &= ~(0x0040));
    if ((((x86emu_parity_tab[(res & 0xff) / 32] >> ((res & 0xff) % 32)) & 1) == 0)) (_X86EMU_env.x86.spc.FLAGS |= (0x0004)); else (_X86EMU_env.x86.spc.FLAGS &= ~(0x0004));
    return res;
}

/****************************************************************************
REMARKS:
Implements the AAA instruction and side effects.
****************************************************************************/
x86emuu16
aas_word(x86emuu16 d)
{
    x86emuu16 res;

    if ((d & 0xf) > 0x9 || (_X86EMU_env.x86.spc.FLAGS & (0x0010))) {
        d -= 0x6;
        d -= 0x100;
        (_X86EMU_env.x86.spc.FLAGS |= (0x0010));
        (_X86EMU_env.x86.spc.FLAGS |= (0x0001));
    }
    else {
        (_X86EMU_env.x86.spc.FLAGS &= ~(0x0001));
        (_X86EMU_env.x86.spc.FLAGS &= ~(0x0010));
    }
    res = (x86emuu16) (d & 0xFF0F);
    (_X86EMU_env.x86.spc.FLAGS &= ~(0x0080));
    if (res == 0) (_X86EMU_env.x86.spc.FLAGS |= (0x0040)); else (_X86EMU_env.x86.spc.FLAGS &= ~(0x0040));
    if ((((x86emu_parity_tab[(res & 0xff) / 32] >> ((res & 0xff) % 32)) & 1) == 0)) (_X86EMU_env.x86.spc.FLAGS |= (0x0004)); else (_X86EMU_env.x86.spc.FLAGS &= ~(0x0004));
    return res;
}

/****************************************************************************
REMARKS:
Implements the AAD instruction and side effects.
****************************************************************************/
x86emuu16
aad_word(x86emuu16 d)
{
    x86emuu16 l;
    x86emuu8 hb, lb;

    hb = (x86emuu8) ((d >> 8) & 0xff);
    lb = (x86emuu8) ((d & 0xff));
    l = (x86emuu16) ((lb + 10 * hb) & 0xFF);

    (_X86EMU_env.x86.spc.FLAGS &= ~(0x0001));
    (_X86EMU_env.x86.spc.FLAGS &= ~(0x0010));
    (_X86EMU_env.x86.spc.FLAGS &= ~(0x0800));
    if (l & 0x80) (_X86EMU_env.x86.spc.FLAGS |= (0x0080)); else (_X86EMU_env.x86.spc.FLAGS &= ~(0x0080));
    if (l == 0) (_X86EMU_env.x86.spc.FLAGS |= (0x0040)); else (_X86EMU_env.x86.spc.FLAGS &= ~(0x0040));
    if ((((x86emu_parity_tab[(l & 0xff) / 32] >> ((l & 0xff) % 32)) & 1) == 0)) (_X86EMU_env.x86.spc.FLAGS |= (0x0004)); else (_X86EMU_env.x86.spc.FLAGS &= ~(0x0004));
    return l;
}

/****************************************************************************
REMARKS:
Implements the AAM instruction and side effects.
****************************************************************************/
x86emuu16
aam_word(x86emuu8 d)
{
    x86emuu16 h, l;

    h = (x86emuu16) (d / 10);
    l = (x86emuu16) (d % 10);
    l |= (x86emuu16) (h << 8);

    (_X86EMU_env.x86.spc.FLAGS &= ~(0x0001));
    (_X86EMU_env.x86.spc.FLAGS &= ~(0x0010));
    (_X86EMU_env.x86.spc.FLAGS &= ~(0x0800));
    if (l & 0x80) (_X86EMU_env.x86.spc.FLAGS |= (0x0080)); else (_X86EMU_env.x86.spc.FLAGS &= ~(0x0080));
    if (l == 0) (_X86EMU_env.x86.spc.FLAGS |= (0x0040)); else (_X86EMU_env.x86.spc.FLAGS &= ~(0x0040));
    if ((((x86emu_parity_tab[(l & 0xff) / 32] >> ((l & 0xff) % 32)) & 1) == 0)) (_X86EMU_env.x86.spc.FLAGS |= (0x0004)); else (_X86EMU_env.x86.spc.FLAGS &= ~(0x0004));
    return l;
}

/****************************************************************************
REMARKS:
Implements the ADC instruction and side effects.
****************************************************************************/
x86emuu8
adc_byte(x86emuu8 d, x86emuu8 s)
{
    register x86emuu32 res; /* all operands in native machine order */
    register x86emuu32 cc;

    if ((_X86EMU_env.x86.spc.FLAGS & (0x0001)))
        res = 1 + d + s;
    else
        res = d + s;

    if (res & 0x100) (_X86EMU_env.x86.spc.FLAGS |= (0x0001)); else (_X86EMU_env.x86.spc.FLAGS &= ~(0x0001));
    if ((res & 0xff) == 0) (_X86EMU_env.x86.spc.FLAGS |= (0x0040)); else (_X86EMU_env.x86.spc.FLAGS &= ~(0x0040));
    if (res & 0x80) (_X86EMU_env.x86.spc.FLAGS |= (0x0080)); else (_X86EMU_env.x86.spc.FLAGS &= ~(0x0080));
    if ((((x86emu_parity_tab[(res & 0xff) / 32] >> ((res & 0xff) % 32)) & 1) == 0)) (_X86EMU_env.x86.spc.FLAGS |= (0x0004)); else (_X86EMU_env.x86.spc.FLAGS &= ~(0x0004));

    /* calculate the carry chain  SEE NOTE AT TOP. */
    cc = (s & d) | ((~res) & (s | d));
    if ((((cc >> 6) ^ ((cc >> 6)>>1)) & 0x1)) (_X86EMU_env.x86.spc.FLAGS |= (0x0800)); else (_X86EMU_env.x86.spc.FLAGS &= ~(0x0800));
    if (cc & 0x8) (_X86EMU_env.x86.spc.FLAGS |= (0x0010)); else (_X86EMU_env.x86.spc.FLAGS &= ~(0x0010));
    return (x86emuu8) res;
}

/****************************************************************************
REMARKS:
Implements the ADC instruction and side effects.
****************************************************************************/
x86emuu16
adc_word(x86emuu16 d, x86emuu16 s)
{
    register x86emuu32 res; /* all operands in native machine order */
    register x86emuu32 cc;

    if ((_X86EMU_env.x86.spc.FLAGS & (0x0001)))
        res = 1 + d + s;
    else
        res = d + s;

    if (res & 0x10000) (_X86EMU_env.x86.spc.FLAGS |= (0x0001)); else (_X86EMU_env.x86.spc.FLAGS &= ~(0x0001));
    if ((res & 0xffff) == 0) (_X86EMU_env.x86.spc.FLAGS |= (0x0040)); else (_X86EMU_env.x86.spc.FLAGS &= ~(0x0040));
    if (res & 0x8000) (_X86EMU_env.x86.spc.FLAGS |= (0x0080)); else (_X86EMU_env.x86.spc.FLAGS &= ~(0x0080));
    if ((((x86emu_parity_tab[(res & 0xff) / 32] >> ((res & 0xff) % 32)) & 1) == 0)) (_X86EMU_env.x86.spc.FLAGS |= (0x0004)); else (_X86EMU_env.x86.spc.FLAGS &= ~(0x0004));

