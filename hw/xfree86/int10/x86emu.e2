        case 5:
            offset = fetch_long_imm();
            if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf2("[%08x]",offset);
            return offset;
        case 6:
            if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf("[ESI]");
            return _X86EMU_env.x86.spc.SI.I32_reg.e_reg;
        case 7:
            if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf("[EDI]");
            return _X86EMU_env.x86.spc.DI.I32_reg.e_reg;
