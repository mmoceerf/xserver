        return;
    seg = fetch_data_word_abs(0, iv * 4);
    off = fetch_data_word_abs(0, iv * 4 + 2);
    printk("%04x:%04x ", seg, off);
}

void
X86EMU_dump_memory(x86emuu16 seg, x86emuu16 off, x86emuu32 amt)
{
    x86emuu32 start = off & 0xfffffff0;
    x86emuu32 end = (off + 16) & 0xfffffff0;
    x86emuu32 i;

    while (end <= off + amt) {
        printk("%04x:%04x ", seg, start);
        for (i = start; i < off; i++)
            printk("   ");
        for (; i < end; i++)
            printk("%02x ", fetch_data_byte_abs(seg, i));
        printk("\n");
        start = end;
        end = start + 16;
    }
}

void
x86emu_single_step(void)
{
    char s[1024];
    int ps[10];
    int ntok;
    int cmd;
    int done;
    int segment;
    int offset;
    static int breakpoint;
    static int noDecode = 1;

    if ((_X86EMU_env.x86.debug & 0x000010)) {
        if (_X86EMU_env.x86.saved_ip != breakpoint) {
            return;
        }
        else {
            _X86EMU_env.x86.debug &= ~0x008000;
            _X86EMU_env.x86.debug |= 0x000002;
            _X86EMU_env.x86.debug &= ~0x000010;
            print_decoded_instruction();
            X86EMU_trace_regs();
        }
    }
    done = 0;
    offset = _X86EMU_env.x86.saved_ip;
    while (!done) {
        printk("-");
        (void)fgets(s, 1023, 
# 278 "./../x86emu/debug.c" 3 4
                            stdin
# 278 "./../x86emu/debug.c"
                                 );
        cmd = parse_line(s, ps, &ntok);
        switch (cmd) {
        case 'u':
            disassemble_forward(_X86EMU_env.x86.saved_cs, (x86emuu16) offset, 10);
            break;
        case 'd':
            if (ntok == 2) {
                segment = _X86EMU_env.x86.saved_cs;
                offset = ps[1];
                X86EMU_dump_memory(segment, (x86emuu16) offset, 16);
                offset += 16;
            }
            else if (ntok == 3) {
                segment = ps[1];
                offset = ps[2];
                X86EMU_dump_memory(segment, (x86emuu16) offset, 16);
                offset += 16;
            }
            else {
                segment = _X86EMU_env.x86.saved_cs;
                X86EMU_dump_memory(segment, (x86emuu16) offset, 16);
                offset += 16;
            }
            break;
        case 'c':
            _X86EMU_env.x86.debug ^= 0x000400;
            break;
        case 's':
            _X86EMU_env.x86.debug ^= 0x000020 | (0x000020|0x000080|0x000100) | 0x000200;
            break;
        case 'r':
            X86EMU_trace_regs();
            break;
        case 'x':
            X86EMU_trace_xregs();
            break;
        case 'g':
            if (ntok == 2) {
                breakpoint = ps[1];
                if (noDecode) {
                    _X86EMU_env.x86.debug |= 0x008000;
                }
                else {
                    _X86EMU_env.x86.debug &= ~0x008000;
                }
                _X86EMU_env.x86.debug &= ~0x000002;
                _X86EMU_env.x86.debug |= 0x000010;
                done = 1;
            }
            break;
        case 'q':
            _X86EMU_env.x86.debug |= 0x010000;
            return;
        case 'P':
            noDecode = (noDecode) ? 0 : 1;
            printk("Toggled decoding to %s\n", (noDecode) ? "FALSE" : "TRUE");
            break;
        case 't':
        case 0:
            done = 1;
            break;
        }
    }
}

int
X86EMU_trace_on(void)
{
    return _X86EMU_env.x86.debug |= 0x000004 | 0x000001 | 0x000002;
}

int
X86EMU_trace_off(void)
{
    return _X86EMU_env.x86.debug &= ~(0x000004 | 0x000001 | 0x000002);
}

static int
parse_line(char *s, int *ps, int *n)
{
    int cmd;

    *n = 0;
    while (*s == ' ' || *s == '\t')
        s++;
    ps[*n] = *s;
    switch (*s) {
    case '\n':
        *n += 1;
        return 0;
    default:
        cmd = *s;
        *n += 1;
    }

    while (1) {
        while (*s != ' ' && *s != '\t' && *s != '\n')
            s++;

        if (*s == '\n')
            return cmd;

        while (*s == ' ' || *s == '\t')
            s++;

        sscanf(s, "%x", &ps[*n]);
        *n += 1;
    }
}



