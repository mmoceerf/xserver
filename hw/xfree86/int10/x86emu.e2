    switch ((sib >> 3) & 0x07) {
    case 0:
        if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf("[EAX");
        i = _X86EMU_env.x86.gen.A.I32_reg.e_reg;
        break;
    case 1:
        if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf("[ECX");
        i = _X86EMU_env.x86.gen.C.I32_reg.e_reg;
        break;
    case 2:
        if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf("[EDX");
        i = _X86EMU_env.x86.gen.D.I32_reg.e_reg;
        break;
    case 3:
        if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf("[EBX");
        i = _X86EMU_env.x86.gen.B.I32_reg.e_reg;
        break;
    case 4:
        i = 0;
        break;
    case 5:
        if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf("[EBP");
        i = _X86EMU_env.x86.spc.BP.I32_reg.e_reg;
        break;
    case 6:
        if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf("[ESI");
        i = _X86EMU_env.x86.spc.SI.I32_reg.e_reg;
        break;
    case 7:
        if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf("[EDI");
        i = _X86EMU_env.x86.spc.DI.I32_reg.e_reg;
        break;
    }
    scale = 1 << ((sib >> 6) & 0x03);
    if (((sib >> 3) & 0x07) != 4) {
        if (scale == 1) {
            if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf("]");
        }
        else {
            if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf2("*%d]",scale);
        }
    }
    return base + (i * scale);
}

