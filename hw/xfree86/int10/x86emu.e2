/*
 *
 * return offset from the SIB Byte
 */
x86emuu32
decode_sib_address(int sib, int mod)
{
    x86emuu32 base = 0, i = 0, scale = 1;

    switch (sib & 0x07) {
    case 0:
        if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf("[EAX]");
        base = _X86EMU_env.x86.gen.A.I32_reg.e_reg;
        break;
    case 1:
        if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf("[ECX]");
        base = _X86EMU_env.x86.gen.C.I32_reg.e_reg;
        break;
    case 2:
        if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf("[EDX]");
        base = _X86EMU_env.x86.gen.D.I32_reg.e_reg;
        break;
    case 3:
        if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf("[EBX]");
        base = _X86EMU_env.x86.gen.B.I32_reg.e_reg;
        break;
    case 4:
        if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf("[ESP]");
        base = _X86EMU_env.x86.spc.SP.I32_reg.e_reg;
        _X86EMU_env.x86.mode |= 0x00000001;
        break;
    case 5:
        if (mod == 0) {
            base = fetch_long_imm();
            if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf2("%08x",base);
        }
        else {
            if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf("[EBP]");
            base = _X86EMU_env.x86.spc.SP.I32_reg.e_reg;
            _X86EMU_env.x86.mode |= 0x00000001;
        }
        break;
    case 6:
        if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf("[ESI]");
        base = _X86EMU_env.x86.spc.SI.I32_reg.e_reg;
        break;
    case 7:
        if ((_X86EMU_env.x86.debug & 0x000001)) x86emu_decode_printf("[EDI]");
        base = _X86EMU_env.x86.spc.DI.I32_reg.e_reg;
        break;
