    {
 /*
	 * In the case of a local connection, we need to get the
	 * host name for authentication.
	 */

 char hostnamebuf[256];
 int len = _XSERVTransGetHostname (hostnamebuf, sizeof hostnamebuf);

 if (len > 0) {
     if (*addrp && *addrlenp < (len + 1))
     {
  free (*addrp);
  *addrp = ((void *)0);
     }
