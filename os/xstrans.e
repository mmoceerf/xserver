 int len = sizeof(saddr.sin_addr.s_addr);
 char *cp = (char *) &saddr.sin_addr.s_addr;

 memcpy (&saddr, *addrp, sizeof (struct sockaddr_in));

 if ((len == 4) && (cp[0] == 127) && (cp[1] == 0) &&
     (cp[2] == 0) && (cp[3] == 1))
 {
     *familyp=256;
 }
 else
